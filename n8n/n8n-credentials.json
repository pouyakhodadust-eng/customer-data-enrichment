{
  "credentials": {
    "postgresApi": {
      "name": "PostgreSQL",
      "type": "postgres",
      "data": {
        "host": "postgres",
        "port": 5432,
        "database": "enrichment_db",
        "user": "enrichment_user",
        "password": "secure_password",
        "ssl": "disable"
      }
    },
    "gmailApi": {
      "name": "Gmail",
      "type": "gmailOAuth2",
      "data": {
        "clientId": "YOUR_GMAIL_CLIENT_ID",
        "clientSecret": "YOUR_GMAIL_CLIENT_SECRET",
        "refreshToken": "YOUR_GMAIL_REFRESH_TOKEN"
      }
    },
    "slackApi": {
      "name": "Slack",
      "type": "slackApi",
      "data": {
        "token": "xoxb-YOUR_SLACK_TOKEN"
      }
    },
    "httpHeaderAuth": {
      "name": "API Auth",
      "type": "httpHeaderAuth",
      "data": {
        "authHeader": "Authorization",
        "authHeaderValue": "Bearer YOUR_API_TOKEN"
      }
    }
  }
}
